      <Fields>
        <Field Name="Server">
          <DataField>Server</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>CentralUtility</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT srvs.[Server], /*ISNULL(srvs.[DNS],'Unknown') AS [DNS],*/ dbs.Name
FROM [CentralUtility].[Documentation].[Databases] dbs
INNER JOIN [CentralUtility].[Lookup].[Servers] srvs ON [srvs].[ServerID] = [dbs].[ServerID]
INNER JOIN [CentralUtility].[Lookup].[Environments] envs ON [envs].[EnvironmentID] = [dbs].[EnvironmentID]
WHERE envs.[Environment] = 'Production'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
